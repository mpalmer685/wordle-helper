{
  "name": "wordle-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@mrpalmer/prettier-config",
  "eslintConfig": {
    "extends": [
      "@mrpalmer",
      "@mrpalmer/eslint-config/jest",
      "@mrpalmer/eslint-config/jsx-a11y",
      "@mrpalmer/eslint-config/react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-throw-literal": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.25",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "clsx": "^1.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "tiny-invariant": "^1.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@mrpalmer/eslint-config": "file:../../projects/mp-tools/packages/eslint-config/mrpalmer-eslint-config-1.0.0.tgz",
    "@mrpalmer/prettier-config": "file:../../projects/mp-tools/packages/prettier-config/mrpalmer-prettier-config-1.0.0.tgz",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/tailwindcss": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  }
}
